package com.chainsys.assignment1;

public class ShopManagement {
	int quantity;
	int total = quantity * 100;

		public static void main(String[] args) {
			ShopManagement sm = new ShopManagement();
			System.out.println(sm.total);
			if(sm.total > 1000) {
				float discount = (sm.total * 10) / 100;
				float amount = sm.total - discount
			}

		}
}
